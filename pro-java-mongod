 @Transactional(readOnly = true, rollbackFor = Exception.class)
    public Map queryIssuingScaleTotal(Map params){
        Map objMap = Maps.newHashMap();
        List<Map> strings = mergeBondMapper.queryIssuingScaleTotal(params);
        objMap.put("toTalSize", strings.size());
        objMap.put("issuingScaleTotal",strings.stream()
                                    .map(item->item.get("ISSUINGSCALE"))
                                    .filter(Objects::nonNull)
                                    .map(String::valueOf)
                                    .mapToDouble(Double::parseDouble)
                                    .sum());
        return objMap;
    }

    @Transactional(readOnly = true, rollbackFor = Exception.class)
    public List<String> findReginal(Map params){
        return  mergeBondMapper.queryRegionalList(params).stream()
                .filter(Objects::nonNull)
                .map(str-> convertService.convertRegional(str))
                .collect(Collectors.toList());
    }
